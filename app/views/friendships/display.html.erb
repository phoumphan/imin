<%= render :partial => '/users/user_nav' %>
<div class="user_profile_container">
  <div class="user_profile">
    <div id="wrapper">

    <div id="friends">
        <%= render :partial => "list_friends" %>
    </div>

    <h2>Find a Friend:</h2>
    <% form_for :search, {:controller => 'friendship', :action => 'display'} do |f|%>
        <%= f.text_field :query %>
        <%= submit_tag "Search" %>
    <% end %>

    <%if @results.empty?%>
      <%if @searchfor%>
        <br />
        <b>Your Search For '<%=@searchfor%>' Returned 0 Results</b>
      <%end%>
    <%else%>
      <div id="table">
        <%= render :partial => "search_users_table" %>
      </div>
    <%end%>
    </div>
  </div>
</div>