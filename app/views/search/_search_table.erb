<div id="about" style= "width: 700px">
<table id="searchTable">

  <thead>
    <tr>      
      <th><a href="">Event</a></th>            
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% for result in @shown_results %>
      </br>
      <tr>
        <td>

          <div style="border-bottom: 1px dotted black; width: 500px;">
            <br />
           <%= link_to result.name, event_path(result.id) %><br /><br />
           Tags: <%result.eventtypes.each do |et|%>
                   <%=et.description%>
                 <%end%><br /><br />
          </div>
        </td>                        
      </tr>
    <%end%>
  </tbody>
</table>

</div>
